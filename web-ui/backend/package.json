{
  "name": "@flowforge/plugin-manager",
  "version": "1.0.0",
  "description": "FlowForge Plugin Manager - Manages ForgeHook container lifecycle",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node scripts/run-migration.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "fastify": "^4.25.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/websocket": "^8.3.0",
    "dockerode": "^4.0.2",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "dotenv": "^16.3.0",
    "uuid": "^9.0.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.0",
    "ajv": "^8.12.0",
    "axios": "^1.6.0",
    "tar-stream": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/dockerode": "^3.3.23",
    "@types/uuid": "^9.0.0",
    "@types/pg": "^8.10.0",
    "@types/tar-stream": "^3.1.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
